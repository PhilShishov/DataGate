@model DataGate.Web.ViewModels.Entities.EntitiesViewModel

<div>
    <table class="table" name="Entities" hidden>
        <thead>
            <tr>
                @foreach (var row in Model.Headers)
                {
                    foreach (var col in row)
                    {
                        <th>
                            <h5>
                                @Html.TextBoxFor(h => col,
                                    new
                                         {
                                        @readonly = true
                                    })
                            </h5>
                        </th>
                    }
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model.Entities)
            {
                <tr>
                    @foreach (var col in row)
                    {
                        <td>
                            @Html.TextBoxFor(c => col,
                                new
                                     {
                                    @readonly = true
                                })
                        </td>
                    }
                </tr>
            }
        </tbody>
    </table>
</div>